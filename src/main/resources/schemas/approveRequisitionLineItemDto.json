{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "RequisitionLineItemDto",
  "description": "A single requisitionLineItemDto",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "orderable": {
      "type": "object",
      "$ref": "basicOrderableDto.json",
      "title": "orderable"
    },
    "approvedQuantity": {
      "type": "integer",
      "title": "approvedQuantity"
    },
    "pricePerPack": {
      "type": "number",
      "title": "pricePerPack"
    },
    "totalCost": {
      "type": "number",
      "title": "totalCost"
    },
    "skipped": {
      "type": "boolean",
      "title": "skipped"
    }
  },
  "required": ["id", "approvedQuantity", "orderable"]
}