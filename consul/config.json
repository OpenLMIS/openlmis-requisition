{
  "service": {
    "Name": "requisition",
    "Id": "requisition-service",
    "Port": 8080,
    "Tags": ["openlmis-service"],
    "check": {
      "interval": "10s",
      "http": "http://HOST:PORT/actuator/health",
      "timeout": "5s",
      "deregisterCriticalServiceAfter": "10m"
    }
  },
  "raml": "/consul/api-definition.yaml",
  "path": [
    "requisition",
    "requisition/docs",
    "requisition/docs/<all>",
    "requisition/webjars",
    "requisition/webjars/<all>"
  ]
}
